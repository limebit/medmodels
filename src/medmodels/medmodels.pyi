from typing import List, Optional
from polars import DataFrame

class Medrecord:
    nodes: List[str]
    edges: List[tuple[str, str]]
    groups: List[str]

    def __init__(self) -> None: ...
    def from_nodes_and_edges(
        nodes: List[tuple[str, object]], edges: List[tuple[str, str, object]]
    ) -> Medrecord: ...
    def from_dataframes(
        nodes_dataframe: DataFrame,
        edges_dataframe: DataFrame,
        node_index_column_name: str,
        node_attribute_column_names: List[str],
        edge_from_index_column_name: str,
        edge_to_index_column_name: str,
        edge_attribute_column_names: List[str],
    ) -> Medrecord: ...
    def node_count(self) -> int: ...
    def edge_count(self) -> int: ...
    def group_count(self) -> int: ...
    def node(self, *node_id: str) -> List[tuple[str, object]]: ...
    def edges_between(self, start_node_id: str, end_node_id: str) -> List[object]: ...
    def group(self, *group: str) -> List[tuple[str, object]]: ...
    def add_nodes(self, nodes: List[tuple[str, object]]) -> None: ...
    def add_edges(self, edges: List[tuple[str, str, object]]) -> None: ...
    def add_group(self, group: str, node_ids_to_add: Optional[List[str]]) -> None: ...
    def remove_group(self, group: str) -> None: ...
    def remove_from_group(self, group: str, node_id: str) -> None: ...
    def add_to_group(self, group: str, node_id: str) -> None: ...
    def neighbors(self, node_id: str) -> List[tuple[str, object]]: ...
